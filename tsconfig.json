{
  "compilerOptions": {
    "target": "ES6",
    "lib": ["es6", "dom"],
    "module": "commonjs",
    "strict": true,
    "noImplicitAny": false,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "types": ["cypress"],
    "baseUrl": "./",
    "paths": {
      "@support/*": ["cypress/support/*"]
    }
  },
  "include": [
    "cypress/**/*.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}
